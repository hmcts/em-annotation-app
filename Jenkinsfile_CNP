#!groovy

properties([
    [
        $class: 'GithubProjectProperty',
        displayName: 'Evidence Management Store App',
        projectUrlStr: 'https://github.com/hmcts/em-annotation-app'
    ],
    pipelineTriggers([
        [$class: 'GitHubPushTrigger']
    ])
])

@Library("Infrastructure")

def type = "java"
def product = "em"
def component = "anno-app"

def channel = '#dm-pipeline'

withPipeline(type , product, component) {
    env.IDAM_S2S_BASE_URI="http://betaDevBccidamS2SLB.reform.hmcts.net"

    after('checkout') {
        echo '${product}-${component} out'
    }
    enableSlackNotifications(channel)
}
